{
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src":"user-guide/en",
        "dest": "en/learn",
        "files": [
          "**/**.{md,yml}",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src":"user-guide/no",
        "dest": "no/learn",
        "files": [
          "**/**.{md,yml}",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src":"user-guide/da",
        "dest": "da/learn",
        "files": [
          "**/**.{md,yml}",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src":"user-guide/se",
        "dest": "se/learn",
        "files": [
          "**/**.{md,yml}",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src":"user-guide/de",
        "dest": "de-learn",
        "files": [
          "**/**.{md,yml}",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src":"user-guide/nl",
        "dest": "nl/learn",
        "files": [
          "**/**.{md,yml}",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "src":"user-guide",
        "dest":"media",
        "files": [
          "images/**",
          "media/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.pdf"
        ]
      },
      {
        "src":"common/icons/",
        "dest":"media",
        "files": "**/*.png"
      },
      {
        "files": [ "assets/**" ]
      }
    ],
    "xref": [
      "No"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "./templates/superoffice"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "SuperOffice User Guide",
      "_appFooter": "Â© SuperOffice. All rights reserved.",
      "_appLogoPath": "assets/images/logo.svg",
      "_appFaviconPath": "assets/images/favicon.ico",
      "_enableSearch": false
    }
  }
}
