{
  "build": {
    "content": [
      {
        "src":"user-guide/en",
        "dest":"en/learn/",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude": [ "**/includes/**" ]
      }
    ],
    "resource": [
      {
        "src":"user-guide",
        "dest":"media",
        "files": [
          "images/**",
          "media/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.pdf"
        ]
      },
      {
        "src":"common/icons/",
        "dest":"media",
        "files": "**/*.png"
      },
      {
        "files": [ "assets/**" ]
      }
    ],
    "xref": [
      "No"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "./build-docs/templates/superoffice",
      "../templates/superoffice"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "SuperOffice User Guide",
      "_appFooter": "Â© SuperOffice. All rights reserved.",
      "_appLogoPath": "assets/images/logo.svg",
      "_appFaviconPath": "assets/images/favicon.ico",
      "_enableSearch": false
    }
  }
}
