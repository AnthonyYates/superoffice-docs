{
  "build": {
    "content": [
      {
        "src":"user-guide/en",
        "dest": "help/en",
        "files": [
          "**/**.{md,yml}",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "media/**",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.xlsx",
          "**/*.zip",
          "**/*.dll",
          "**/*.docx",
          "**/*.pdf",
          "**/*.pptx",
          "**/*.app"
        ]
      }
    ],
    "xref": [
      "No"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "./templates/superoffice"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "SuperOffice Help Files",
      "_appFooter": "Â© SuperOffice. All rights reserved.",
      "_appLogoPath": "assets/images/logo.svg",
      "_appFaviconPath": "assets/images/favicon.ico",
      "_enableSearch": false
    }
  }
}
